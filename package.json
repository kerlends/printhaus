  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "@material-ui/icons": "^2.0.0",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "dotenv": "^6.0.0",
    "gatsby": "^1.9.277",
    "gatsby-image": "^1.0.55",
    "gatsby-link": "^1.6.46",
    "gatsby-plugin-manifest": "^1.0.27",
    "gatsby-plugin-netlify": "^1.0.21",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sharp": "^1.6.48",
    "gatsby-plugin-typography": "^1.7.19",
    "gatsby-source-shopify": "https://github.com/kerlends/gatsby-source-shopify#workaround",
    "gatsby-transformer-sharp": "^1.6.27",
    "numeral": "^2.0.6",
    "polymorph-js": "^0.2.2",
    "popmotion": "^8.3.3",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "shopify-buy": "^1.6.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "*postbuild": "bash ./scripts/postbuild.sh",
    "*prebuild": "rm -rf ./.cache",
    "develop": "gatsby develop",
    "*predevelop": "rm -rf ./.cache",
    "start": "yarn develop -H 0.0.0.0",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "flow-bin": "^0.77.0",
    "prettier": "^1.13.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "prettier": {
    "printWidth": 64,
    "arrowParens": "always",
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "graphql": true
    }
  }
}
